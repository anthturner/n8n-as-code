{
  "name": "n8n-as-code",
  "displayName": "n8n as code",
  "description": "Edit your n8n workflows directly in VS Code with AI assistance. Synchronization bridge for n8n.",
  "version": "0.0.1",
  "publisher": "etienne-lescot",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other",
    "Data Science",
    "Machine Learning"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "n8n-as-code.helloWorld",
        "title": "Hello n8n"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/node": "18.x",
    "typescript": "^5.1.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/EtienneLescot/n8n-as-code"
  }
}